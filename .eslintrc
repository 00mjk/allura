{
  "rules": {
    "indent": [
      2,
      4
    ]
  },
  "globals": {
    "ReactDOM": true,
    "$": true,
    "window": true,
    "_": true,
    "ReactReorderable": true
  },
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "browser": true
  },
  "extends": "google",
  "ecmaFeatures": {
    "jsx": true,
    "forOf": true,
    "experimentalObjectRestSpread": true
  },
  "plugins": [
    "react"
  ]
}